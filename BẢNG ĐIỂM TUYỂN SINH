package code_ptit;
import java.text.DecimalFormat;
import java.util.*;
public class NewClass1{
    public static class TuyenSinh{
        String ma, ht;
        double toan, ly, hoa;
        public TuyenSinh(String ma, String ht, double toan, double ly, double hoa){
            this.ma = ma;
            this.ht = ht;
            this.toan = toan;
            this.ly = ly;
            this.hoa = hoa;
        }
        public double uuTien(){
            String kv = ma.substring(0,3);
            if(kv.equals("KV1"))
                return 0.5;
            if(kv.equals("KV2"))
                return 1.0;
            if(kv.equals("KV3"))
                return 2.5;
            return 0;
        }
        public double tong(){
            return toan* 2 + ly + hoa;
        }
        public String trangThai(){
            double x = tong() + uuTien();
            if(x < 24)
                return "TRUOT";
            return "TRUNG TUYEN";
        }
        @Override
        public String toString(){
            DecimalFormat df = new DecimalFormat("#.#");
            return ma + ' ' + ht + ' ' + df.format(uuTien()) + ' ' + df.format(tong()) + ' ' + trangThai();
        }
    }
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        ArrayList<TuyenSinh> ds = new ArrayList<>();
        int n = sc.nextInt();
        sc.nextLine();
        while(n-- > 0){
            String ma = sc.nextLine();
            String ten = sc.nextLine();
            double toan = sc.nextDouble();
            double ly = sc.nextDouble();
            double hoa = sc.nextDouble();
            sc.nextLine();
            ds.add(new TuyenSinh(ma, ten, toan, ly, hoa));
        }
        for(TuyenSinh i : ds)
            System.out.println(i);
    }
}
