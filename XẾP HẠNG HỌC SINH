package code_ptit;
import java.util.*;
public class NewClass {
    public static class HocSinh{
        String ma, ht;
        double dtb;
        int rank;
        public HocSinh(String ma, String ht, double dtb){
            this.ma = ma;
            this.ht = ht;
            this.dtb = dtb;
        }

        public String getMa() {
            return ma;
        }

        public String getHt() {
            return ht;
        }

        public double getDtb() {
            return dtb;
        }

        public int getRank() {
            return rank;
        }
        
        public String xepLoai(){
            if(dtb < 5)
                return "Yeu";
            if(dtb < 7)
                return "Trung Binh";
            if(dtb < 9)
                return "Kha";
            return "Gioi";
        }
        @Override
        public String toString(){
            return ma + " " + ht + " " + dtb + " " + xepLoai() +  " " + rank;
        }
    }
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        ArrayList<HocSinh> ds = new ArrayList<>();
        int n = sc.nextInt();
        sc.nextLine();
        for(int i = 0; i < n; i++){
            String ma = String.format("HS%02d", i+1);
            String t = sc.nextLine();
            double d = sc.nextDouble();
            sc.nextLine();
            ds.add(new HocSinh(ma, t, d));
        }
        Collections.sort(ds, new Comparator<HocSinh>(){
            @Override
            public int compare(HocSinh a, HocSinh b){
                return Double.compare(b.dtb,a.dtb);
            }
        });
        double diemCur = -1;
        int rankCur = 0;
        for(int i = 0; i < n; i++){
            if(ds.get(i).getDtb() == diemCur)
                ds.get(i).rank = rankCur;
            else{
                ds.get(i).rank = i + 1;
                rankCur = i + 1;
                diemCur = ds.get(i).dtb;
            }    
        }
        Collections.sort(ds, new Comparator<HocSinh>(){
            @Override
            public int compare(HocSinh a, HocSinh b){
                return a.ma.compareTo(b.ma);
            }
        });
        for (HocSinh hs : ds) {
            System.out.println(hs);
        }
    }
}
