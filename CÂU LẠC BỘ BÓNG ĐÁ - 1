package code_ptit;
import java.util.*;
public class NewClass {
    public static class BongDa{
        String ma, ten;
        long gia;
        public BongDa(String ma, String ten, long gia){
            this.ma = ma;
            this.ten = ten;
            this.gia = gia;
        }
    }
    public static class CLB{
        String ma, ten;
        long dt;
        public CLB(String ma, String ten, long dt){
            this.ma = ma;
            this.ten = ten;
            this.dt = dt;
        }
        @Override
        public String toString(){
            return ma + ' ' + ten + ' ' + dt;
        }
    }
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        ArrayList<BongDa> ds1 = new ArrayList<>();
        int n = sc.nextInt();
        sc.nextLine();
        while(n-- > 0){
            String ma = sc.nextLine();
            String ten = sc.nextLine();
            long gia = sc.nextLong();
            sc.nextLine();
            ds1.add(new BongDa(ma, ten, gia));
        }
        ArrayList<CLB> ds2 = new ArrayList<>();
        int m = sc.nextInt();
        sc.nextLine();
        while(m-- > 0){
            String ma = sc.next();
            long sl = sc.nextLong();
            sc.nextLine();
            String tmp = ma.substring(1,3);
            for(int i = 0; i < ds1.size(); i++){
                if (tmp.equals(ds1.get(i).ma)){
                    long dt = ds1.get(i).gia * sl;
                    ds2.add(new CLB(ma, ds1.get(i).ten, dt));
                }
            }
        }
        for(CLB it : ds2)
            System.out.println(it);
    }
}
