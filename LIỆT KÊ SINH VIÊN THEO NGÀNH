package code_ptit;
import java.util.*;
class SinhVien{
    private String ma, ht, lop, email;
    public SinhVien(String ma, String ht, String lop, String email){
        this.ma = ma;
        this.ht = ht;
        this.lop = lop;
        this.email = email;
    }
    public String getMa(){
        return ma;
    }
    public String getLop(){
        return lop;
    }
    @Override
    public String toString(){
        return ma + " " + ht + " " + lop + " " + email;
    }
}
public class NewClass{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        ArrayList<SinhVien> ds = new ArrayList<>();
        int t = sc.nextInt();
        sc.nextLine();
        while(t-- > 0){
            String ma = sc.nextLine();
            String ht = sc.nextLine();
            String lop = sc.nextLine();
            String email = sc.nextLine();
            SinhVien sv = new SinhVien(ma, ht, lop, email);
            ds.add(sv);
        }
        int k = sc.nextInt();
        sc.nextLine();
        while(k-- > 0){
            String l = sc.nextLine();
            System.out.println("DANH SACH SINH VIEN NGANH " + l.toUpperCase() + ":");
            for(SinhVien it : ds){
                String tmp = it.getMa().substring(3, 7);
                if(l.equals("Ke toan") && tmp.equals("DCKT"))
                    System.out.println(it);
                else if(l.equals("Cong nghe thong tin") && tmp.equals("DCCN") && it.getLop().charAt(0) != 'E')
                    System.out.println(it);
                else if(l.equals("An toan thong tin") && tmp.equals("DCAT") && it.getLop().charAt(0) != 'E')
                    System.out.println(it);
                else if(l.equals("Vien thong") && tmp.equals("DCVT"))
                    System.out.println(it);
                else if(l.equals("Dien tu") && tmp.equals("DCDT"))
                    System.out.println(it);
            }
        }
    }
}
