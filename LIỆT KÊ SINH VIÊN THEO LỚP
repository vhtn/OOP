package code_ptit;
import java.util.*;
class SinhVien{
    private String ma, ht, lop, email;
    public SinhVien(String ma, String ht, String lop, String email){
        this.ma = ma;
        this.ht = ht;
        this.lop = lop;
        this.email = email;
    }
    public String getLop(){
        return lop;
    }
    @Override
    public String toString(){
        return ma + " " + ht + " " + lop + " " + email;
    }
}
public class NewClass{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        ArrayList<SinhVien> ds = new ArrayList<>();
        int t = sc.nextInt();
        sc.nextLine();
        while(t-- > 0){
            String ma = sc.nextLine();
            String ht = sc.nextLine();
            String lop = sc.nextLine();
            String email = sc.nextLine();
            SinhVien sv = new SinhVien(ma, ht, lop, email);
            ds.add(sv);
        }
        int k = sc.nextInt();
        sc.nextLine();
        while(k-- > 0){
            String l = sc.nextLine();
            System.out.println("DANH SACH SINH VIEN LOP " + l + ":");
            for(SinhVien it : ds){
                if(it.getLop().equals(l))
                    System.out.println(it);
            }
        }
    }
}
